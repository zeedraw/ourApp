<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.administrator.ourapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppBaseTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MissionInfo"></activity>
        <activity android:name=".MyAccount"></activity>
        <activity android:name=".MyMission"></activity>
        <activity android:name=".MySetting"></activity>
        <activity android:name=".SignUp"></activity>
        <activity android:name=".my_task"></activity>
    </application>

</manifest>